#include <bits/stdc++.h>
using namespace std;

bool PowerOfTwo(int n)
{
    if (n == 0)
        return false;
 
    return (ceil(log2(n)) == floor(log2(n)));
}
 

int main()
{
   int x; 
   cin>>x; 
   
    PowerOfTwo(x) ? cout << "Yes" << endl
                     : cout << "No" << endl;
  
    return 0;
}
